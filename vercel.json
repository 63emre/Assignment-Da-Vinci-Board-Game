{
  "version": 2,
  "buildCommand": "cd project/frontend && npm install && npm run build",
  "outputDirectory": "project/frontend/dist",
  "routes": [
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}