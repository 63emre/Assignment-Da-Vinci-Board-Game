{
  "version": 2,
  "buildCommand": "npm run install:all && cd project/frontend && npm run build",
  "outputDirectory": "project/frontend/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/project/backend/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "project/backend/api/index.ts": {
      "runtime": "@vercel/node"
    }
  }
}