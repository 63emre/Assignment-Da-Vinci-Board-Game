{
  "version": 2,
  "builds": [
    {
      "src": "project/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "project/backend/api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/project/backend/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/project/frontend/dist/$1"
    }
  ],
  "functions": {
    "project/backend/api/index.ts": {
      "maxDuration": 30
    }
  }
}